<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Title</th>
        <th>Total Hours</th>
        <th>Earning per hour</th>
      </tr>
    </thead>
    <tbody>
      <% projects.each do |project| %>
        <tr>
          <td><%= project[0].humanize %></td>
          <td><%= project[1] %></td>
          <td><%= project[2] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
